<form method="post" action="{{ path('form_delete', {'id': formulaire.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ formulaire.id) }}">
    <button type="submit"  class="btn btn-primary">Delete</button>
    
</form>
